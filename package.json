{
  "name": "hosii",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:dev && run-p dev:*",
    "dev:client": "next dev -p 8000",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "dev:pathpida": "pathpida --watch",
    "build": "run-p build:client build:server",
    "prebuild:client": "npm run generate",
    "build:client": "pathpida && aspida && next build",
    "build:types": "pathpida && aspida && npm run build:frourio --prefix server",
    "build:server": "npm run build --prefix server",
    "generate": "run-p generate:*",
    "generate:aspida": "aspida",
    "generate:server": "npm run generate --prefix server",
    "lint": "eslint --ext .ts,.js,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "migrate:dev": "npm run migrate:dev --prefix server",
    "migrate:reset": "npm run migrate:reset --prefix server",
    "start": "run-p start:*",
    "start:client": "next start",
    "start:server": "npm start --prefix server",
    "pretest": "npm run generate",
    "test": "jest",
    "pretypecheck": "npm run generate",
    "typecheck": "tsc --noEmit && tsc --noEmit -p server",
    "check": "npm run typecheck && npm run lint"
  },
  "dependencies": {
    "@aspida/fetch": "^1.9.0",
    "@aspida/node-fetch": "^1.6.3",
    "@aspida/swr": "^1.8.0",
    "@chakra-ui/react": "^1.5.2",
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "class-validator": "^0.13.1",
    "framer-motion": "^6.2.8",
    "next": "^12.1.0",
    "node-fetch": "^3.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.27.1",
    "swr": "^1.2.2"
  },
  "devDependencies": {
    "@aspida/node-fetch": "^1.6.3",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/node": "^14.14.31",
    "@types/node-fetch": "^3.0.3",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.5",
    "eslint-plugin-react": "^7.29.4",
    "fastify": "^3.27.4",
    "fastify-cors": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.15.2",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.7",
    "ts-node": "^9.1.1",
    "typescript": "~4.5.5"
  }
}
